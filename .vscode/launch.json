{
    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm run dev",
            "name": "Run npm run dev",
            "type": "node-terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}",           
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        }
    ]
   
    
}